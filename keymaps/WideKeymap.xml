<keymap version="1" name="WideKeymap" parent="Default for XWin">
  <action id="Back">
    <keyboard-shortcut first-keystroke="ctrl alt left" />
    <mouse-shortcut keystroke="button4" />
    <keyboard-shortcut first-keystroke="ctrl alt semicolon" />
  </action>
  <action id="EditorPasteFromX11" />
  <action id="Forward">
    <keyboard-shortcut first-keystroke="ctrl alt right" />
    <mouse-shortcut keystroke="button5" />
    <keyboard-shortcut first-keystroke="ctrl alt quote" />
  </action>
  <action id="Generate">
    <keyboard-shortcut first-keystroke="alt insert" />
    <keyboard-shortcut first-keystroke="ctrl comma" />
  </action>
  <action id="GotoDeclaration">
    <keyboard-shortcut first-keystroke="ctrl b" />
    <mouse-shortcut keystroke="control button1" />
    <mouse-shortcut keystroke="button2" />
  </action>
  <action id="NewElement">
    <keyboard-shortcut first-keystroke="alt insert" />
    <keyboard-shortcut first-keystroke="ctrl comma" />
  </action>
  <action id="NewScratchFile">
    <keyboard-shortcut first-keystroke="shift ctrl alt insert" />
    <keyboard-shortcut first-keystroke="ctrl alt comma" />
  </action>
</keymap>